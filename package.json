    "lodash-decorators": "^4.4.1",
    "moment": "^2.19.1",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.1.0",
    "pea-cli": "^0.5.11",
    "pinch-zoom-proper": "^1.0.0",
    "prop-types": "^15.5.10",
